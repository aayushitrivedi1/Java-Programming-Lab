
import java.util.Scanner;
public class Main
{

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
System.out.println(&quot;Enter a
number:&quot;);
int n=sc.nextInt();
int fact=1;
for(int i=1;i&lt;=n;i++)
{
fact*=i;
}
System.out.println(&quot;Factorial of
number is&quot;+fact);

}
}
