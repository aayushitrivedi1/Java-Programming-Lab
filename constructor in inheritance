class Animal {  
 String name;   Animal(String name) {   this.name = name;  
 }  
}  
class Dog extends Animal {  
 String breed;  
 Dog(String name, String breed) {   super(name);   this.breed = breed;  
 }  
 @Override   public String toString() {   return "Dog{" +  
 "name='" + name + '\'' +  
 ", breed='" + breed + '\'' +  
 '}';  
 }  
}  
public class Main {  
 public static void main(String[] args) {  
 System.out.println("AAYUSHI TRIVEDI"); 
System.out.println("0827CI221005");
 Dog dog = new Dog("Fido", "Golden Retriever"); 
 System.out.println(dog); 
 }} 
