public class CommandLineArguments {  public static void main(String[] args){  if(args.length == 0){ 
 System.out.println("AAYUSHI TRIVEDI"); 
System.out.println("0827CI221005");
 System.out.println("No command-line argument"); 
 }else{ 
 System.out.println("command-line argument");  for(int i=0;i<args.length;i++){ 
 System.out.println("argument"+(i+1)+":"+args[i]); 
 } 
 } 
 } 
} 
