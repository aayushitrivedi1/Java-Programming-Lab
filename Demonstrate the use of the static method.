class StaticTest {  // static method  static int add(int a, int b){  return a + b; 
 } 
} 
public class Main { 
 public static void main( String[] args ) {  StaticTest st = new StaticTest(); 
 System.out.println("AAYUSHI TRIVEDI"); 
System.out.println("0827CI221005");
 System.out.println(" 2 + 3 = " + StaticTest.add(2,3)); 
 } 
} 
